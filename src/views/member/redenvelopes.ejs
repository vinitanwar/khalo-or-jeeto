    <script>
    
    function loadHistory (){
        $.ajax({
          type: "GET",
          url: "/api/webapi/giftredeemlist",
          dataType: "json",
          success: function (response) {
            let html = `<div class="gift-history-container">
                          <div class="gift-history-header">
                            <svg
                              xmlns="http://www.w3.org/2000/svg"
                              width="20px"
                              height="20px"
                              viewBox="0 0 24 24"
                            >
                              <path
                                fill="#f95959"
                                d="M21 11.11V5a2 2 0 0 0-2-2h-4.18C14.4 1.84 13.3 1 12 1s-2.4.84-2.82 2H5c-1.1 0-2 .9-2 2v14a2 2 0 0 0 2 2h6.11c1.26 1.24 2.98 2 4.89 2c3.87 0 7-3.13 7-7c0-1.91-.76-3.63-2-4.89M12 3c.55 0 1 .45 1 1s-.45 1-1-1s-1-.45-1-1s.45-1 1-1M6 7h12v2H6zm3.08 10H6v-2h3.08c-.05.33-.08.66-.08 1s.03.67.08 1M6 13v-2h5.11c-.61.57-1.07 1.25-1.43 2zm10 8c-2.76 0-5-2.24-5-5s2.24-5 5-5s5 2.24 5 5s-2.24 5-5 5m.5-4.75l2.86 1.69l-.75 1.22L15 17v-5h1.5z"
                              ></path>
                            </svg>
                            <span>History</span>
                          </div>
                          <div class="gift-history-content">`;
        
            response.datas.forEach(element => {
              html += `<div class="gift-history-item">
                          <div style="gap: 5px; display: flex; flex-direction: column;">
                            <span class="gift-sts">Successfully Received</span>
                            <span class="gft-dt">${element.time}</span>
                          </div>
                          <div class="gift-money" style="border: .5px solid #fb7073;padding: 2px;border-radius:3px;">
                            <img
                              src="data:image/png;base64,"
                              alt="wallet icon"
                              style="width: 10px;"
                            />
                            <span style="color:#ff9903;">â‚¹${element.money}.00</span>
                          </div>
                       </div>`;
            });
        
            html += `</div></div>`;
        
            $('.infiniteScroll').html(html);
            console.log(response);
          },
          error: function (xhr, status, error) {
            console.log('Error fetching data:', error);
          }
        });
    }
    
    loadHistory();
        function alertMess(text, sic) {
            $('body').append(
                `
                <div data-v-1dcba851="" class="msg">
                    <div data-v-1dcba851="" class="msg-content v-enter-active v-enter-to" style=""> ${text} </div>
                </div>
                `
            );
            setTimeout(() => {
                $('.msg .msg-content').removeClass('v-enter-active v-enter-to');
                $('.msg .msg-content').addClass('v-leave-active v-leave-to');
                setTimeout(() => {
                    $('.msg').remove();
                }, 100);
                sic.removeClass('block-click');
            }, 1500);
        }
        $('button').click(function (e) { 
            e.preventDefault();
            let value = $('input').val().trim();
            $(this).addClass('block-click');
            let iej = $(this);
            if (value) {
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/use/redenvelope",
                    data: {
                        code: value,
                    },
                    dataType: "json",
                    success: function (response) {
                        alertMess(response.message, iej);
                         loadHistory();
                    }
                });
            } else {
				alertMess('Please enter the gift code', iej);
            }
        });
        // 
        //infiniteScroll
        
        
                
        
        
        
    </script>
