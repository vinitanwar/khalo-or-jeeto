            <div class="card">
                <div class="card-body">
                  <h5 class="card-title">Salary Records Table</h5>
                  <table class="table" id="tableget">
                    <thead>
                      <tr>
                        <!-- <th>#</th> -->
                        <th class="text-center">Phone</th>
                        <th class="text-center">Amount</th>
                        <th class="text-center">Type</th>
                        <th class="text-center">Time</th>
